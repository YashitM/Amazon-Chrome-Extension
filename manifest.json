{
	"name": "Amazon Buy ",
	"description": "Keep a track of how much you've spent on Amazon so far!",
	"version": "1.0",

	"manifest_version": 2,

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"tabs",
		"https://ajax.googleapis.com/"
	]

}